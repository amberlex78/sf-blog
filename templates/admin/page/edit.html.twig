{% extends 'admin/layouts/body_card.html.twig' %}

{% block title %}Edit Page{% endblock %}

{% block breadcrumbs %}
    {% include 'admin/_embed/_card/_breadcrumbs.html.twig' with {
        'indexAction': path('admin_page_index'), 'indexTitle': 'Pages', 'actionTitle': 'Edit'
    } %}
{% endblock %}

{% block actions %}
    {% include 'admin/_embed/_card/_actions.html.twig' with {
        'pathAdd': path('admin_page_new'),
        'pathShow': path('admin_page_show', {'id': page.id}),
        'pathDelete': path('admin_page_delete', {'id': page.id}), 'entityTile': page.title, 'entityId': page.id,
    } %}
{% endblock %}

{% block content %}
    {% include'admin/page/_form.html.twig' %}
{% endblock %}
